#A = input()
A = 141700

for i in range(len(A) :
    if A[i]=='0'
    t.penup()
    t.goto(i*100+40-200,0)        
    t.pendown()
    for j in range(len(zero)):
        if zero[j][2] == 1:
            t.penup()
        else: t.pendown()
        t.right(zero[j][1])
        t.forward(zero[j][0])
               
